<%= @user.email %>

<%= form_for @picture, :html => { :multipart => true } do |form| %>
  <%= form.hidden_field :user_id, value: @user.id %>

  <%= form.file_field :file %>
  <%= form.submit 'Submit' %>
<% end %>

<h2>All Pictures</h2>

<% @user.pictures.each do |picture| %>
	<%= image_tag picture.file.url(:medium) %>
	
<% end %>
